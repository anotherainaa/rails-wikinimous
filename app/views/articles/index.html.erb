
<h1>Wikinimous</h1>
<%= link_to "Create a article", new_article_path %>

<div class="container">
  <ul class="list-unstyled"><% @articles.each do |article| %>
      <div class="card text-center">
       <li>
        <h2><%= link_to article.title, article_path(article)%></h2>
        <p><%= article.content[0..200]  %></p>
        </li>
      <% end %>
      </div>
  </ul>
</div>

